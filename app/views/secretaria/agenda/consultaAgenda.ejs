  <div id="container-veragenda">
    <div id="titulo">
      <h1>CONSULTAR AGENDA</h1>
    </div>
    <div class="formulario" align="center">
      <form id="consultAgend">
        <div class="row">
          <label for="nomePaciente">
            <input type="text" class="inputs" id="nomePaciente" placeholder="Nome do Paciente">
          </label>
        </div>
        <div class="row">
          
        </div>
        <div class="row">
            <br>
        </div>
        <div class="row">
          <label for="adicionar">
            <input type="submit" class="btnSubmit" name="agendar" value="Agendar">
          </label>
          <br><br>
          <font size="2" face="Courier New" >
          <table border="1" > 
            <tr>
             
              <td>Nome</td>
              <td>Horario</td>
              <td>Dia</td>
              <td>exercicio</td>
            </tr>
            
            <% for(var i = 0; i < consultaAgenda.length ; i++ ) { %>
            <tr >
              
              <td><%= consultaAgenda[i].nome %></td>
              <td><%= consultaAgenda[i].horario %></td>
              <td><%= consultaAgenda[i].dia %></td>
              <td><%= consultaAgenda[i].exercicio %></td>
            </tr>
            <% } %>
          </table>
        </font>
        </div>

      </form>
    </div>

